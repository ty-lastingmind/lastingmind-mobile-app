    "expo-font": "~13.3.2",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "lottie-react-native": "7.2.2",
    "nativewind": "^4.1.23",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.0.0",
    "react-hook-form": "^7.60.0",
    "react-native": "0.79.5",
