    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/drawer": "^7.5.7",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "~53.0.17",
    "expo-audio": "~0.4.8",
    "expo-build-properties": "~0.14.8",
