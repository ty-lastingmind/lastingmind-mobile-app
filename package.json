{
  "name": "lasting-mind-rn",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean:folders": "rm -rf ios && rm -rf android",
    "format:check": "prettier --check '**/*.{ts,tsx,md,json}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,md,json}'",
    "lint:check": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint lint --fix",
    "type:check": "tsc",
    "type:check-watch": "tsc --watch",
    "codequalty:check": "npm run format:fix && npm run lint:check && npm run type:check",
    "prepare": "husky",
    "api:generate": "orval --config ./orval.config.ts",
    "api:watch": "orval --config ./orval.config.ts --watch",
    "api:clean": "rm -rf src/libs/api/generated.ts src/libs/api/model",
    "postinstall": "npm run api:generate"
  },
  "dependencies": {
