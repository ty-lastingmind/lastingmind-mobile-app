{
  "openapi": "3.1.0",
  "info": {
    "title": "LastingMind API",
    "version": "0.1.0"
  },
  "paths": {
    "/journal/pull-past-journal-entries": {
      "get": {
        "tags": ["Journal"],
        "summary": "Pull Past Journal",
        "description": "Pull the past journal entries",
        "operationId": "pull_past_journal_journal_pull_past_journal_entries_get",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PastEntriesResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/journal/submit-journal-entry": {
      "post": {
        "tags": ["Journal"],
        "summary": "Submit Journal Entry",
        "operationId": "submit_journal_entry_journal_submit_journal_entry_post",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmitJournalInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResponseSubmissionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/journal/pull-journal-details": {
      "post": {
        "tags": ["Journal"],
        "summary": "Pull Journal Details",
        "operationId": "pull_journal_details_journal_pull_journal_details_post",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/JournalDetailsInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/JournalDetailsResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/utils/pull-suggested-topics": {
      "get": {
        "tags": ["Utils"],
        "summary": "Pull Suggested Topics",
        "description": "Pull the suggested topics for a user",
        "operationId": "pull_suggested_topics_utils_pull_suggested_topics_get",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuggestedTopicsResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/utils/generate-new-suggested-topics": {
      "post": {
        "tags": ["Utils"],
        "summary": "Generate New Suggested Topics",
        "description": "Creates new suggested topics for a user",
        "operationId": "generate_new_suggested_topics_utils_generate_new_suggested_topics_post",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateSuggestedTopicsInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SuggestedTopicsResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/utils/transcribe-audio": {
      "post": {
        "tags": ["Utils"],
        "summary": "Transcribe Audio",
        "description": "Creates new suggested topics for a user",
        "operationId": "transcribe_audio_utils_transcribe_audio_post",
        "parameters": [
          {
            "name": "authorization",
            "in": "header",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Authorization"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TranscribeAudioInput"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TranscriptionResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/login/verify-user": {
      "post": {
        "tags": ["Login"],
        "summary": "Login User",
        "description": "Pull the past journal entries",
        "operationId": "login_user_login_verify_user_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ExampleChatData": {
        "properties": {
          "question": {
            "type": "string",
            "title": "Question"
          },
          "answer": {
            "type": "string",
            "title": "Answer"
          }
        },
        "type": "object",
        "required": ["question", "answer"],
        "title": "ExampleChatData"
      },
      "FirstAnswerData": {
        "properties": {
          "percent_increase": {
            "type": "number",
            "title": "Percent Increase"
          },
          "response_type": {
            "$ref": "#/components/schemas/ResponseType"
          }
        },
        "type": "object",
        "required": ["percent_increase", "response_type"],
        "title": "FirstAnswerData"
      },
      "GenerateSuggestedTopicsInput": {
        "properties": {
          "userFullName": {
            "type": "string",
            "title": "Userfullname"
          },
          "currentTopics": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Currenttopics"
          }
        },
        "type": "object",
        "required": ["userFullName"],
        "title": "GenerateSuggestedTopicsInput"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HighLevelPastEntry": {
        "properties": {
          "title": {
            "type": "string",
            "title": "Title"
          },
          "date": {
            "type": "string",
            "format": "date-time",
            "title": "Date"
          },
          "responseId": {
            "type": "string",
            "title": "Responseid"
          }
        },
        "type": "object",
        "required": ["title", "date", "responseId"],
        "title": "HighLevelPastEntry"
      },
      "JournalDetailsInput": {
        "properties": {
          "responseId": {
            "type": "string",
            "title": "Responseid"
          }
        },
        "type": "object",
        "required": ["responseId"],
        "title": "JournalDetailsInput"
      },
      "JournalDetailsResponse": {
        "properties": {
          "topic": {
            "type": "string",
            "title": "Topic"
          },
          "date": {
            "type": "string",
            "format": "date-time",
            "title": "Date"
          },
          "text": {
            "type": "string",
            "title": "Text"
          },
          "audioFiles": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Audiofiles"
          }
        },
        "type": "object",
        "required": ["topic", "date", "text"],
        "title": "JournalDetailsResponse"
      },
      "LevelUpData": {
        "properties": {
          "level_name": {
            "type": "string",
            "title": "Level Name"
          }
        },
        "type": "object",
        "required": ["level_name"],
        "title": "LevelUpData"
      },
      "LoginRequest": {
        "properties": {
          "email": {
            "type": "string",
            "title": "Email"
          },
          "password": {
            "type": "string",
            "title": "Password"
          }
        },
        "type": "object",
        "required": ["email", "password"],
        "title": "LoginRequest"
      },
      "MilestoneData": {
        "properties": {
          "milestone": {
            "type": "integer",
            "title": "Milestone"
          }
        },
        "type": "object",
        "required": ["milestone"],
        "title": "MilestoneData"
      },
      "NextPageResponse": {
        "properties": {
          "next_page": {
            "$ref": "#/components/schemas/NextPageType"
          },
          "next_page_data": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/LevelUpData"
              },
              {
                "$ref": "#/components/schemas/FirstAnswerData"
              },
              {
                "$ref": "#/components/schemas/MilestoneData"
              },
              {
                "$ref": "#/components/schemas/PercentIncreaseData"
              },
              {
                "$ref": "#/components/schemas/ExampleChatData"
              },
              {
                "$ref": "#/components/schemas/SavedAnswerData"
              }
            ],
            "title": "Next Page Data"
          }
        },
        "type": "object",
        "required": ["next_page", "next_page_data"],
        "title": "NextPageResponse"
      },
      "NextPageType": {
        "type": "string",
        "enum": [
          "level_up_page",
          "first_answer_page",
          "journal_milestone",
          "question_milestone",
          "interview_milestone",
          "percent_increase",
          "example_chat",
          "saved_answer"
        ],
        "title": "NextPageType"
      },
      "PastEntriesResponse": {
        "properties": {
          "entries": {
            "items": {
              "$ref": "#/components/schemas/HighLevelPastEntry"
            },
            "type": "array",
            "title": "Entries"
          }
        },
        "type": "object",
        "required": ["entries"],
        "title": "PastEntriesResponse"
      },
      "PercentIncreaseData": {
        "properties": {
          "percent_increase": {
            "type": "number",
            "title": "Percent Increase"
          }
        },
        "type": "object",
        "required": ["percent_increase"],
        "title": "PercentIncreaseData"
      },
      "ResponseSubmissionResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status",
            "default": "success"
          },
          "next_page_info": {
            "$ref": "#/components/schemas/NextPageResponse"
          }
        },
        "type": "object",
        "required": ["next_page_info"],
        "title": "ResponseSubmissionResponse"
      },
      "ResponseType": {
        "type": "string",
        "enum": ["journal_entry", "curated_questions", "interview"],
        "title": "ResponseType"
      },
      "SavedAnswerData": {
        "properties": {
          "response_type": {
            "$ref": "#/components/schemas/ResponseType"
          }
        },
        "type": "object",
        "required": ["response_type"],
        "title": "SavedAnswerData"
      },
      "SubmitJournalInput": {
        "properties": {
          "topic": {
            "type": "string",
            "title": "Topic"
          },
          "date": {
            "type": "string",
            "format": "date-time",
            "title": "Date"
          },
          "text": {
            "type": "string",
            "title": "Text"
          },
          "audioFiles": {
            "anyOf": [
              {
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Audiofiles"
          }
        },
        "type": "object",
        "required": ["topic", "date", "text"],
        "title": "SubmitJournalInput"
      },
      "SuggestedTopicsResponse": {
        "properties": {
          "suggested_topics": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Suggested Topics"
          }
        },
        "type": "object",
        "required": ["suggested_topics"],
        "title": "SuggestedTopicsResponse"
      },
      "TranscribeAudioInput": {
        "properties": {
          "userFullName": {
            "type": "string",
            "title": "Userfullname"
          },
          "gcsUri": {
            "type": "string",
            "title": "Gcsuri"
          },
          "mimeType": {
            "type": "string",
            "enum": [
              "audio/mp3",
              "audio/mpga",
              "audio/wav",
              "audio/webm",
              "audio/m4a",
              "audio/opus",
              "audio/aac",
              "audio/flac",
              "audio/pcm",
              "audio/mp4"
            ],
            "title": "Mimetype"
          }
        },
        "type": "object",
        "required": ["userFullName", "gcsUri", "mimeType"],
        "title": "TranscribeAudioInput"
      },
      "TranscriptionResponse": {
        "properties": {
          "transcript": {
            "type": "string",
            "title": "Transcript"
          }
        },
        "type": "object",
        "required": ["transcript"],
        "title": "TranscriptionResponse"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
